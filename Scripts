Tab:CreateToggle({
	Name = "Auto Attack/Click",
	CurrentValue = false,
	Flag = "Toggle1", 
	Callback = function(Value)
		a = Value
            while a do task.wait()
				local args = {
				[1] = {
					[1] = "Hit",
					[2] = getClosest().Parent
				}
			}

			game:GetService("ReplicatedStorage").Remotes.Server:FireServer(unpack(args))
		end
	end,
})
